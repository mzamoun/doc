function setupProfileChart(username){$.get('/backend/requests/profile/profile_stats.php?user='+username,function(resp){$('.loaderForChart').hide();var resp=JSON.parse(resp);console.log(resp);var ctx_1=$('.skill_graph .linechart');var ctx_1_labels=resp.mod_data.labels;var ctx_1_data={};ctx_1_data.label='Points';ctx_1_data.pointRadius=2;ctx_1_data.borderWidth=2;ctx_1_data.borderColor='rgba(134,210,156,0.8)';ctx_1_data.backgroundColor='rgba(217,251,227,0.7)';ctx_1_data.fill=!0;ctx_1_data.lineTension=0;ctx_1_data.data=resp.mod_data.scores;var myBarChart=new Chart(ctx_1,{type:'line',data:{labels:ctx_1_labels,datasets:[ctx_1_data]},options:{legend:{display:!1},scales:{xAxes:[{ticks:{fontSize:10},gridLines:{color:"#f3f3f3",}}],yAxes:[{ticks:{fontSize:10},gridLines:{color:"#f3f3f3",}}]}}})})}