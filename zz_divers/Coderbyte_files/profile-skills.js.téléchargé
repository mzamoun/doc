$('.skill_assessment_explanation').click(function(){window.location.href='https://coderbyte.com/about'});function createSkillBars(spanClassText){var spans='';var fillLevel=0;var text='';var fillText;if(spanClassText==='expert'){text='Expert';fillLevel=4;fillText='green_fill_level_expert'}
if(spanClassText==='advanced'){text='Advanced';fillLevel=3;fillText='green_fill_level_advanced'}
if(spanClassText==='intermediate'){text='Intermediate';fillLevel=2;fillText='green_fill_level_intermediate'}
for(var i=0;i<4;i++){if(fillLevel>0){spans+='<span class="'+fillText+'"></span>\n'}else{spans+='<span class="no_fill"></span>\n'}
fillLevel--}
return text+' '+spans}
function setupLanguageLogo(language){var langChosen=window.__MAIN_LANGUAGE_LIST[language]?window.__MAIN_LANGUAGE_LIST[language]:'JavaScript';$('.languageLogo').attr('src',langChosen.icon);$('.languageLogo').load(function(){$('.languageLogo').removeClass('spinner')})}
function skillAssessmentSetup(data,isCandidate){if(data){console.log('skills',data);var usedLanguages=Object.keys(data.languages);var bestLanguage=null;var levels=[21,12,5];if(isCandidate){var levels=[10,7,3]}
var levelTypes=['expert','advanced','intermediate'];for(var i=0;i<levels.length;i++){if(data.hard_challenges[1]>=levels[i]){var maxScore=(data.hard_challenges[1]*15);var threshold=Math.floor(maxScore-(maxScore*0.1));var thresholdSmaller=Math.floor(maxScore-(maxScore*0.3));if(data.hard_challenges[0]>=threshold){var content=createSkillBars(levelTypes[i]);$('.skill_algs p').html(content);break}else if(data.medium_challenges[0]>=thresholdSmaller){var content=createSkillBars('intermediate');$('.skill_algs p').html(content);break}}}
for(var i=0;i<levels.length;i++){if(data.medium_challenges[1]>=levels[i]){var maxScore=(data.medium_challenges[1]*10);var threshold=Math.floor(maxScore-(maxScore*0.1));var thresholdSmaller=Math.floor(maxScore-(maxScore*0.3));if(data.medium_challenges[0]>=threshold){var content=createSkillBars(levelTypes[i]);$('.skill_datastruct p').html(content);break}else if(data.medium_challenges[0]>=thresholdSmaller){var content=createSkillBars('intermediate');$('.skill_datastruct p').html(content);break}}}
for(var i=0;i<usedLanguages.length;i++){if(!bestLanguage||data.languages[usedLanguages[i]][0]>bestLanguage[0]){bestLanguage=data.languages[usedLanguages[i]];bestLanguage.push(usedLanguages[i])}}
setupLanguageLogo(bestLanguage[2]);$('.skill_lang h3').html(bestLanguage[2].replace('Cpp','C++').replace('Csharp','C#'));for(var i=0;i<levels.length;i++){if(bestLanguage[1]>=levels[i]){var maxScore=(bestLanguage[1]*10);var threshold=Math.floor(maxScore-(maxScore*0.2));if(bestLanguage[0]>=threshold){var content=createSkillBars(levelTypes[i]);$('.skill_lang p').html(content);break}}}}}